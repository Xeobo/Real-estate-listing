<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="73dp"
        android:layout_marginTop="29dp"
        android:text="Agent:"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView20"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="37dp"
        android:text="Telefon:"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView21"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="38dp"
        android:text="Agent:"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />


    <TextView
        android:id="@+id/user_list_agent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView20"
        android:layout_marginStart="86dp"
        android:layout_toEndOf="@+id/textView20"
        android:text="Agent"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />

    <TextView
        android:id="@+id/user_list_telephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView21"
        android:layout_alignStart="@id/user_list_agent"
        android:layout_toEndOf="@+id/textView20"
        android:text="telephone"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />

    <TextView
        android:id="@+id/user_list_agency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignStart="@id/user_list_agent"
        android:layout_toEndOf="@+id/textView20"
        android:text="Agencija"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:textColor="#000" />

</RelativeLayout>